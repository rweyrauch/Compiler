class Cow 
{    
    int height;
    int weight;

    void Init(int w, int h) 
    {
        weight = w;
        height = h;
    }             

    void Moo() 
    {
        callout("printf", "%d %d\n", this.height, this.weight);
    } 
} 
 
class Program 
{
    void main() 
    {
        Cow betsy;
        betsy = new Cow;
        betsy.Init(100, 122);
        betsy.Moo();
    } 
} 
