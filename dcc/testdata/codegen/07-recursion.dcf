class Program {
    int factorial(int n)
    {
        if (n <= 1) {
            return 1;
        } else {
            return n * factorial(n - 1);
        }
        return 0;
    }

    void main()
    {
        int res;

        res = factorial(10);

        callout("printf", "10! = %d (3628800)\n", res);
    }
}
