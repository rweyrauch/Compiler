class Program
{
    void main()
    {
        int i, sum;
        int j;

        sum = 0;
        for j = 2, 5
        {
            sum += j;
        }
        callout("printf", "sum = %d\n", sum);
        
        sum = 0;
        for j = 0, 10 
        {
			sum = sum + j;
			if (j == 5) 
			{
				callout("printf", "Breaking when j == %d(5).\n", j);
				break;
			}
			callout("printf", "j = %d\n", j);
        }
        callout("printf", "sum = %d\n", sum);
     
        sum = 0;
        for j = 1, 5
        {
            if (j == 2)
            {
				callout("printf", "Continue when j == %d(2).\n", j);
				continue;            
            }
            sum += j;
        }
        callout("printf", "sum = %d\n", sum);
        
    }
}
