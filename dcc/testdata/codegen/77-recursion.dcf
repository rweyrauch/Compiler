class Program {
    int factorial(int n)
    {
        int foo;
        if (n <= 1) 
        {
            return 1;
        } 
        else 
        {
            return (n * factorial(n - 1));
        }
        return 1;
    }

    void main()
    {
        int res;

        res = factorial(10);

        callout("printf", "10! = %d (3628800)\n", res);
    }
}
